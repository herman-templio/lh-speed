{
  "name": "templ-lighthouse",
  "version": "1.0.0",
  "description": "REST API running lighthouse test",
  "main": "src/app.js",
  "type": "module",
  "eslintConfig": {
    "extends": "airbnb/base",
    "rules": {
      "no-console": "off"
    }
  },
  "scripts": {
    "lint": "eslint src/**",
    "start": "NODE_ENV=production node src/app.js",
    "dev": "NODE_ENV=local nodemon --exec node src/app.js",
    "test-cov": "nyc mocha",
    "test": "mocha",
    "watch": "nodemon --exec ./node_modules/.bin/mocha",
    "cliw": "nodemon --exec node ./src/cli.js --",
    "cli": "node ./src/cli.js --"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "enter this"
  },
  "author": {
    "name": "HA",
    "email": "dev@templ.io"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "app-utils": "file:../app-utils",
    "axios": "^0.24.0",
    "dotenv": "^8.2.0",
    "lighthouse": "^9.2.0",
    "puppeteer": "^13.1.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-json-error": "^3.1.2",
    "koa-router": "^10.1.1"
